class Solution {
    public boolean isPalindrome(String s) {
        
        s = s.replaceAll("[^0-9a-zA-Z]", "").toUpperCase();  
        int n = s.length();
        if(n==0)
            return true;
        for(int i=0; i<n/2; i++){
            if(s.charAt(i) != s.charAt(n-i-1))
                return false;
        }
        
        
        return true;
    }
}
